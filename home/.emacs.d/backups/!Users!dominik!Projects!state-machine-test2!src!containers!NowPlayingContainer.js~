const movie = {
  image: 'https://image.tmdb.org/t/p/w500/7WsyChQLEftFiDOVTGkv3hFpyyt.jpg',
  title: 'Avengers: Infinity War',
  year: '2018',
  rating: 76,
  id: '1234',
};

const moviesData = [];

for (let i = 0; i < 8; i++) {
  moviesData.push({...movie, id: movie.id + i});
}

const NowPlayingContainer = ({ render }) => (
  render(moviesData)
);

export default NowPlayingContainer;
